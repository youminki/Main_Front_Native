{
  "expo": {
    "name": "MelpikNative",
    "slug": "melpik-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.liftcomma.melpiknative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.liftcomma.melpiknative"
    },
    "extra": {
      "eas": {
        "projectId": "9d1f401f-2b81-4175-8c6b-dbef6886975e"
      }
    },
    "plugins": ["expo-asset"]
  }
}
